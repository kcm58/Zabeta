<div class="task">
  <form><ul>
    <li>
      <label for="name">Name</label>
      <input type="text" id="name" name="name" />
    </li>
    <li>
      <label for="description">Description</label>
      <textarea id="description" name="description"></textarea>
    </li>
    <li>
      <label for="begin_date">Begin date</label>
      <input type="text" id="begin_date" name="begin_date" />
    </li>
    <li>
      <label for="end_date">End date</label>
      <input type="text" id="end_date" name="end_date" />
    </li>
    <li>
      <label for="owner">Owner</label>
      <input type="text" id="owner" name="owner" />
    </li>
    <li>
      <label for="program">Program</label>
      <input type="text" id="program" name="program" />
    </li>
    <li>
      <label for="response">Response</label>
      <input type="text" id="response" name="response" />
    </li>
    <li>
      <label for="rubric">Rubric</label>
      <input type="text" id="rubric" name="rubric" />
    </li>
  </ul></form>
</div>

<div class="actions">
  <button class="cancel" name="cancel">Cancel</button>
  <button class="update" name="update">Update</button>
</div>
