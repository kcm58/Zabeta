<h2>Semesters</h2>
<table class="list">
  <thead>
    <tr>
      <th>
        Name
      </th>
      <th>
        Program
      </th>
      <th>
      	Edit
      </th>
    </tr>
  </thead>
  <tbody>
    {{#Semester}}
    <tr>
      <td>{{name}}</td>
      <td>{{program}}</td>
      <td><a onclick="edit('{{id}}')" style="cursor:pointer">Edit</a>
    </tr>
    {{/Semester}}
  </tbody>
</table>
<a onclick="addNew('semester')" style="cursor:pointer">Add</a>